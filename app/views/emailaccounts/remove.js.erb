
if(confirm('Are you sure to delete <%= @emailaccount.address %>?')){
    $.ajax({
        type: "POST",
        beforeSend: function(xhr) {xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))},
        url:  "<%= emailaccount_path(@emailaccount) %>",
        data: {"_method":"delete"},
        success: function(result){
            console.log(result);
        }
    });
}else{
    alert('note sure');
}